{
  "Settings": {
    "Clients": [
      {
        "ClientId": "WeatherSiteClient",
        "ClientSecret": "VerySecretSecret",
        "AllowedScopes": ["CitiesApi"]
      }
    ],
    "ApiResources": [
      {
        "Name": "CitiesApi",
        "DisplayName": "Cities API",
        "IsEnabled": true,
        "Scopes": ["CitiesApi"]
      }
    ],
    "Scopes": [
      {
        "Name": "CitiesApi",
        "DisplayName": "Cities API"
      }
    ]
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "ApplicationName": "OAuthServer"
    }
  }
}
