{
  "MongoSettings": {
    "connectionString": "mongodb://localhost:27017",
    "database": "WeatherHistoryDB",
    "CollectionName": "CityWeatherForecasts",
    "seed": false
  },
  "logger": {
    "applicationName": "weather-history-service",
    "excludePaths": [ "/ping", "/metrics" ],
    "level": "warning",
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": false,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": true,
      "url": "http://localhost:5341/"
    }
  },
  "rabbitMq": {
    "connectionName": "weather-history-service",
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "logger": {
      "enabled": true
    },
    "messageProcessor": {
      "enabled": true,
      "type": "distributed",
      "messageExpirySeconds": 300
    },
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:10",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "declare": true,
      "durable": true,
      "autoDelete": false,
      "type": "topic",
      "name": "serviceexchange"
    },
    "queue": {
      "declare": true,
      "durable": true,
      "exclusive": false,
      "autoDelete": false
    },
    "context": {
      "enabled": true,
      "header": "message_context"
    },
    "spanContextHeader": "span_context",
    "deadLetter": {
      "enabled": true,
      "prefix": "dlx-",
      "declare": true
    },
    "maxProducerChannels": 1000
  },
  "AllowedHosts": "*"
}