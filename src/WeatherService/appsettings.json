{
  "ServiceSettings": {
    "OpenWeatherHost": "https://api.openweathermap.org/",
    "ApiKey": "API KEY"
  },
  "RabbitMqSettings": {
    "Host": "rabbitmq://rabbitmq"
  },
  "ConnectionStrings": {
    "RedisConnection": "redis:6379"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "MassTransit": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "ApplicationName": "WeatherService"
    }
  },
    "ResiliencePipelines": {
    "Default": {
      "Name": "default",
      "MaxRetryAttempts": 3,
      "DelaySeconds": 2,
      "UseJitter": true,
      "TimeoutSeconds": 5
    },
    "Health": {
      "Name": "health",
      "MaxRetryAttempts": 0,
      "DelaySeconds": 0,
      "UseJitter": false,
      "TimeoutSeconds": 2
    }
  }
}