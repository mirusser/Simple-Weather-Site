{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.7.1",
      "destination": "wwwroot/lib/jquery",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "twitter-bootstrap@4.6.2",
      "destination": "wwwroot/lib/bootstrap",
      "files": [
        "css/bootstrap.min.css",
        "js/bootstrap.bundle.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "admin-lte@3.2.0",
      "destination": "wwwroot/lib/admin-lte",
      "files": [
        "dist/css/adminlte.min.css",
        "dist/js/adminlte.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "@fortawesome/fontawesome-free@5.15.4",
      "destination": "wwwroot/lib/fontawesome",
      "files": [
        "css/all.min.css",
        "webfonts/fa-solid-900.woff2",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-brands-400.woff2"
      ]
    },
    {
      "library": "select2@4.1.0-rc.0",
      "destination": "wwwroot/lib/select2",
      "files": [
        "css/select2.min.css",
        "js/select2.full.min.js"
      ]
    },
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/lib/jquery-validation",
      "files": [
        "jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/signalr",
      "files": [
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}
