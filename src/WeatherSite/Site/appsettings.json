{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "ApplicationName": "WeatherSite"
    }
  },
  "ConnectionStrings": {
    "RedisConnection": "localhost:6379"
  },
  "ApiEndpoints": {
    "WeatherServiceApiUrl": "http://localhost:5850/api/weatherforecast/",
    "CitiesServiceApiUrl": "http://localhost:5040/api/city/",
    "WeatherHistoryServiceApiUrl": "http://localhost:8783/api/CityWeatherForecast/",
    "IconServiceApiUrl": "http://localhost:5960/api/Icons/",
    "SignalRServer": "http://localhost:5670/"
  },
  "GrpcCitiesClientConnections":{
    "Uri": "http://localhost:5030"
  },
  "ApiConsumerAuthSettings": {
    "AuthorityUrl": "http://localhost:5185",
    "ClientId": "WeatherSiteClient",
    "ClientSecret": "VerySecretSecret",
    "Scope": "CitiesApi"
  }
}
