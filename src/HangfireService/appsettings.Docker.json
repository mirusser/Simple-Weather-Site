{
  "MongoSettings": {
    "ConnectionString": "mongodb://mongo:27017",
    "Database": "HangfireDB"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "ApplicationName": "HangfireService"
    }
  },
  "RabbitMQSettings": {
    "Host": "rabbitmq://rabbitmq"
  },
  "AllowedHosts": "*"
}
