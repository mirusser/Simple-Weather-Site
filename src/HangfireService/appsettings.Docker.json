{
  "MongoSettings": {
    "ConnectionString": "mongodb://mongo:27017/?replicaSet=rs0",
    "Database": "HangfireDB"
  },
  "RabbitMQSettings": {
    "Host": "rabbitmq://rabbitmq"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "ApplicationName": "HangfireService"
    }
  },
  "ResiliencePipelines": {
    "Default": {
      "Name": "default",
      "MaxRetryAttempts": 3,
      "DelaySeconds": 2,
      "UseJitter": true,
      "TimeoutSeconds": 10
    },
    "Health": {
      "Name": "health",
      "MaxRetryAttempts": 0,
      "DelaySeconds": 0,
      "UseJitter": false,
      "TimeoutSeconds": 2
    }
  }
}
